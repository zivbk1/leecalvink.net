@font-face{font-family:'Equity';src:url("/fonts/equity/a_regular.woff2")}@font-face{font-family:'Equity';font-style:italic;src:url("/fonts/equity/a_italic.woff2")}@font-face{font-family:'Equity';font-style:bold;src:url("/fonts/equity/a_bold.woff2")}@font-face{font-family:'Concourse';src:url("/fonts/concourse/4_medium.woff2")}@font-face{font-family:'Concourse';font-style:italic;src:url("/fonts/concourse/4_medium_italic.woff2")}@font-face{font-family:'Concourse';font-style:bold;src:url("/fonts/concourse/7_bold.woff2")}@font-face{font-family:'Triplicate';src:url("/fonts/concourse/a_code_regular.woff2")}@font-face{font-family:'Triplicate';font-style:italic;src:url("/fonts/concourse/a_code_italic.woff2")}@font-face{font-family:'Triplicate';font-style:bold;src:url("/fonts/concourse/a_code_bold.woff2")}html{--main-font: Equity;--sans-font: Concourse;--mono-font: Triplicate}body{font-size:1em;font-family:var(--main-font);margin:0}h1,h2,h3{margin:0}p{margin:0}abbr{font-variant:all-small-caps}img{max-width:100%}svg{width:1em;height:2ex}ol,ul{margin:0}time{font-family:var(--sans-font)}sup{font-feature-settings:'sups' 1}small{font-family:var(--sans-font);font-size:var(--sfont);color:var(--high-contrast)}button{border:none;cursor:pointer;font-size:var(--sfont);font-family:var(--sans-font)}kbd,samp{font-family:'triplicate'}
